        [[ -n $FZF_FS_SEARCHK_MARGIN ]] && o+="--margin=${FZF_FS_SEARCHK_MARGIN} "

        o+="--tabstop=${FZF_FS_SEARCHK_TABSTOP} "

        FZF_DEFAULT_OPTS=$o
}

