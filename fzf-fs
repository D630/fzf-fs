{
        builtin typeset o

        (( FZF_FS_SET_BLACK )) && o+="--black "
        (( FZF_FS_SET_CYCLE )) && o+="--cycle "
        (( FZF_FS_SET_EXACT )) && o+="--exact "
        (( FZF_FS_SET_EXTENDED )) || o+="--no-extended "
        (( FZF_FS_SET_HSCROLL )) || o+="--no-hscroll "
